[{{repo_name}}]
name={{repo_name}}
{% if repo_baseurl is defined %}baseurl={{repo_baseurl}}
{% endif %}
{% if repo_mirrorlist is defined %}mirrorlist={{repo_mirrorlist}}
{% endif %}
{% if repo_gpgkey is defined %}gpgkey={{repo_gpgkey}}
{% endif %}
gpgcheck={{repo_gpgcheck}}
enabled={{repo_enabled}}

